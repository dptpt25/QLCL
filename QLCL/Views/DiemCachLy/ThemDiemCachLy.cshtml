@{
    ViewData["Title"] = "Thêm điểm Cách ly";
}

<div class="container">
    <form action="/DiemCachLy/InsertDCL" method="post">
        <div class="form-group">
            <label for="MaDiemCachLy">Mã điểm cách ly</label>
            <input type="text" class="form-control" name="MaDiemCL" id="MaDiemCachLy">
        </div>
        <div class="form-group">
            <label for="TenDiemCachLy">Tên điểm cách ly</label>
            <input type="text" class="form-control" name="TenDiemCL" id="TenDiemCachLy">
        </div>
        <div class="form-group">
            <label for="DiaChi">Địa chỉ</label>
            <input type="text" class="form-control" name="DiachiCL" id="DiaChi">
        </div>
        <button type="submit" class="btn btn-primary">Thêm</button>
    </form>
</div>

